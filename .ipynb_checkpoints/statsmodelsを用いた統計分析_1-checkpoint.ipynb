   "source": [
    "train = pizza[:-24]\n",
    "#単純指数平滑法\n",
    "model_simple = SimpleExpSmoothing(train,\n",
    "                              initialization_method=\"estimated\")\n",
    "#Holt法\n",
    "model_holt = Holt(train, \n",
    "                  initialization_method=\"estimated\")\n",
    "#Holt-Winters\n",
    "model_holt_wint = ExponentialSmoothing(train,\n",
    "                                  seasonal_periods=7,\n",
    "                                  trend='add',\n",
    "                                  seasonal='add',\n",
    "                                  initialization_method=\"estimated\")\n",
    "#SARIMAX\n",
    "model_sarimax = sm.tsa.statespace.SARIMAX(train,                                                                \n",
    "                                order=(1, 1, 1),\n",
    "                                seasonal_order=(1, 1, 1, 7),\n",
    "                                enforce_stationarity=False,\n",
    "                                enforce_invertibility=False)\n",
    "model_sarimax2 = sm.tsa.statespace.SARIMAX(train,                                                                \n",
    "                                #order=(1, 1, 1),\n",
    "                                seasonal_order=(1, 1, 1, 7),\n",
    "                                enforce_stationarity=False,\n",
    "                                enforce_invertibility=False)"
   ]
  },
