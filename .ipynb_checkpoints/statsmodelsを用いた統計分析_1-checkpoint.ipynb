   "source": [
    "fit_simple = model_simple.fit(optimized=True)\n",
    "fit_holt = model_holt.fit()\n",
    "fit_holt_wint = model_holt_wint.fit()\n",
    "fit_sarimax = model_sarimax.fit()\n",
    "fit_sarimax2 = model_sarimax2.fit()"
   ]
